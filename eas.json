{
  "cli": {
    "version": ">= 10.2.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
        "WALT_ID_API_KEY": "@walt_id_api_key",
        "WALT_ID_API_URL": "@walt_id_api_url"
      }
    }
  },
  "submit": {
    "development": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}